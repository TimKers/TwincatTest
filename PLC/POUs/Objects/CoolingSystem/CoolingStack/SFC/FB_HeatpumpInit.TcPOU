    <Method Name="ParseHeatPumpErrors" Id="{59c35a10-5b22-4032-94ac-2589b63a9939}">
      <Declaration><![CDATA[METHOD ParseHeatPumpErrors : BOOL
VAR_INPUT
	w_Alarms1	:	WORD;
	w_Alarms2	:	WORD;
	w_Alarms3	:	WORD;
	w_Alarms4	:	WORD;
	w_Alarms5	:	WORD;
	w_Alarms6	:	WORD;
	w_Alarms7	:	WORD;
END_VAR		
VAR_IN_OUT
	get	:	ST_HeatpumpGet;	
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[
get.events.b_ProbeError		:=	WORD_TO_BOOL(w_Alarms3) OR WORD_TO_BOOL(w_Alarms4) OR w_Alarms5.0;

get.events.b_PressureError		:=	w_Alarms1.0 OR	//E001: High Pressure
									w_Alarms1.1 OR //E002: Low Pressure
									w_Alarms2.3;	//E020: Inverted Pressure Transducers


get.events.b_CompressorError	:=	w_Alarms1.2 OR //E003: Compressor overheated
									w_Alarms1.7 OR //E008: Compressor underspeed
									w_Alarms1.8 OR	//E009: High disch temp
									w_Alarms1.12 OR //E013: Comp2 Overheated
									w_Alarms2.2 OR	//E019: CP2 High disch temp
									w_Alarms2.6 OR	//E023: Comp3 Overheated
									w_Alarms5.1 OR //'E801: Link Inverter1',
									w_Alarms5.2 OR //'E802: Link Inverter2',
									w_Alarms5.3 OR //'E803: Link Inverter3',
									w_Alarms5.4 OR //'E851: HWfault Inverter1',
									w_Alarms5.5 OR //'E852: HWfault Inverter2',
									w_Alarms5.6 OR //'E853: HWfault Inverter3',
									w_Alarms5.7 OR //'E861: OverCurrent Inv1',
									w_Alarms5.8 OR //'E862: OverCurrent Inv2',
									w_Alarms5.9 OR //'E863: OverCurrent Inv3',
									w_Alarms5.10 OR //'E871: High Temp Inv1',
									w_Alarms5.11 OR //'E872: High Temp Inv2',
									w_Alarms5.12 OR //'E873: High Temp Inv3',
									w_Alarms5.13 OR //'E881: Bad Voltage Inv1',
									w_Alarms5.14 OR //'E882: Bad Voltage Inv2',
									w_Alarms5.15 OR //'E883: Bad Voltage Inv3',
									w_Alarms6.0 OR //E891: Phase Seq Inv1',
									w_Alarms6.1 OR //'E892: Phase Seq Inv2',
									w_Alarms6.2 OR //'E893: Phase Seq Inv3',
									w_Alarms6.3 OR //'E901: Model err Inv1',
									w_Alarms6.4 OR //'E902: Model err Inv2',
									w_Alarms6.5 OR //'E903: Model err Inv3',
									w_Alarms6.6 OR //'E911: Ovl error Inv1',
									w_Alarms6.7 OR //'E9112: Ovl error Inv2',
									w_Alarms6.8 OR //'E913: Ovl error Inv3',
									w_Alarms6.9 OR //'E921: Overc PFC Inv1',
									w_Alarms6.10 OR //'E922: Overc PFC Inv2',
									w_Alarms6.11 OR //'E923: Overc PFC Inv3',
									w_Alarms6.12 OR //'E931: Int Com ERR Inv1',
									w_Alarms6.13 OR //'E932: Int Com ERR Inv2',
									w_Alarms6.14 OR //'E933: Int Com ERR Inv3',
									w_Alarms6.15 OR //'E941: Fault PFC Inv1',
									w_Alarms7.0 OR //E942: Fault PFC Inv2
									w_Alarms7.1 OR //'E943: Fault PFC Inv3',
									w_Alarms7.2 OR //'E951: Probe error Inv1',
									w_Alarms7.3 OR //'E952: Probe error Inv2',
									w_Alarms7.4 OR //'E953: Probe error Inv3',
									w_Alarms7.5 OR //'E961: Abnormal Cond Inv1',
									w_Alarms7.6 OR //'E962: Abnormal Cond Inv2',
									w_Alarms7.7 OR //'E963: Abnormal Cond Inv3',
									w_Alarms7.8 OR //'E971: Inv1 Eeprom Error',
									w_Alarms7.9 OR //'E972: Inv2 Eeprom Error',
									w_Alarms7.10 OR //'E973: Inv3 Eeprom Error',
									w_Alarms7.11;	//E029: CP3 High disch temp

get.events.b_ValveError		:=		w_Alarms2.11; //E041: Incongruent temps;


get.events.b_FanError			:=	w_Alarms1.2 OR //E004: Fan Overheated	
									w_Alarms1.13 OR //E014: Fan2 Overheated 
									w_Alarms2.7;	//E024: Fan3 Overheated					

get.events.b_PumpError			:=	w_Alarms1.5 OR	 	//E006: Lack of flow
									w_Alarms1.15 OR 	//E016: Pump Overheated
									w_Alarms2.9;		//E026: Pump2 Overheated

get.events.b_WaterTempError		:=	w_Alarms2.1 OR	//E018: OverTemperature
									w_Alarms1.4;	//E005: Frost

get.events.b_IOError			:=	w_Alarms2.14 OR 	//E101: IOMod1 Disconnected
									w_Alarms2.15; 		//E102: IOMod2 Disconnected
]]></ST>
      </Implementation>
    </Method>
