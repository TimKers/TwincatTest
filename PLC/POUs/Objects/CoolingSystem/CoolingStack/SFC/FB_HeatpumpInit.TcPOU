        <ST><![CDATA[
//ModbusStartTrigger(clk := b_EnableTransmission);

IF NOT b_enableTransmission THEN
	b_enableTransmission := TRUE;
END_IF

MBMaster.ReadRegs(	UnitID 		:= 	HeatPumpID,
					Quantity 	:= 	4,
					MBAddr		:=	1,
					cbLength 	:= 	64,
					pMemoryAddr	:=	ADR(DataBuffer),
					Execute		:=	ModbusStartTrigger.Q,
					Timeout		:=	T#250MS,		
					Busy		=>	b_MBBusy,
					Error 		=>	b_MBerr,
					cbRead 		=>	ui_cbRead,
					ErrorId		=>	w_errid
					);


